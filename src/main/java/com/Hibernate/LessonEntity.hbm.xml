<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.Hibernate.LessonEntity" table="lesson" schema="mywork">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="lesson">
            <column name="Lesson" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="mark">
            <column name="mark" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>